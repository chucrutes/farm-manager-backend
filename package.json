  "main": "main.ts",
  "engines": {
    "yarn": "1.22.x",
    "node": "21.x"
  },
  "scripts": {
    "start": "node dist/infra/http/server.js",
    "build": "tsc -p tsconfig.build.json && tsc-alias",
    "dev": "tsx watch src/infra/http/server.ts",
    "test": "vitest --config vitest.config.mts",
    "test:single": "vitest run",
    "seed": "vitest --config vitest.seed.config.mts",
    "prisma:studio": "dotenv -e .env -c -- prisma studio",
    "prisma:generate": "prisma generate dev",
    "prisma:migrate": "dotenv -e .env -c -- prisma migrate dev",
    "prisma:migrate:prod": "dotenv -e .env -c -- prisma migrate deploy",
    "prisma:push": "dotenv -e .env -c -- prisma db push",
    "format": "prettier --write ."
  },
